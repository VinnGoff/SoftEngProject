<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unisa.se.project.controllers.CaricaFileViewController">
   <children>
      <VBox prefHeight="401.0" prefWidth="602.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="500.0" text="Inserisci il nome del file:">
               <VBox.margin>
                  <Insets left="50.0" right="50.0" top="30.0" />
               </VBox.margin>
               <font>
                  <Font name="Cambria Math" size="24.0" />
               </font>
            </Label>
            <TextField>
               <VBox.margin>
                  <Insets left="50.0" right="50.0" top="30.0" />
               </VBox.margin>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </TextField>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="499.0" text="Inserisci il percorso del file:">
               <VBox.margin>
                  <Insets left="50.0" right="50.0" top="30.0" />
               </VBox.margin>
               <font>
                  <Font name="Cambria Math" size="24.0" />
               </font>
            </Label>
            <TextField>
               <VBox.margin>
                  <Insets left="50.0" right="50.0" top="30.0" />
               </VBox.margin>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </TextField>
         </children>
      </VBox>
      <HBox prefHeight="131.0" prefWidth="600.0" AnchorPane.topAnchor="270.0">
         <children>
            <Button fx:id="AnnullaButtonCaricaFile" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="Annulla">
               <HBox.margin>
                  <Insets left="80.0" top="30.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="CaricaButtonCaricaFile" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="Carica">
               <HBox.margin>
                  <Insets left="270.0" top="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
